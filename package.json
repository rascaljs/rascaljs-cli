{
  "name": "@rascaljs/cli",
  "version": "0.0.2",
  "description": "RascalCoder's command lint tools",
  "author": "Rascal-Coder <meno.qiqio@gmail.com> (https://github.com/Rascal-Coder)",
  "license": "MIT",
  "bin": {
    "rascal": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "esno": "esno",
    "lint": "eslint . --fix",
    "update-version": "bumpp package.json",
    "publish-pkg": "pnpm -r publish --access public",
    "release": "pnpm update-version && pnpm publish-pkg",
    "prepare": "husky install",
    "rascal": "node dist/index.js",
    "commit": "pnpm rascal git-commit"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "execa": "5.1.1",
    "kolorist": "^1.6.0",
    "minimist": "^1.2.7",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "eslint-config-rascal": "0.0.10",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4",
    "zx": "^7.1.1"
  }
}
